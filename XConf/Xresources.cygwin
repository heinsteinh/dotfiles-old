! ? Xft options
! normal screen
!Xft.dpi                                    : 96
Xft.dpi                                    : 76
Xft.autohint                               : false
Xft.lcdfilter                              : lcddefault
Xft.hintstyle                              : hintslight
Xft.hinting                                : true
Xft.antialias                              : true
Xft.rgba                                   : none


!! ? Cursor options
 Xcursor.theme                              : aDMZ
 Xcursor.size                               : 16   !  32, 48 or 64 may also be good values
 Xcursor.theme_core                         : true


!! ? Tk theme
 *TkTheme                                   : clam



